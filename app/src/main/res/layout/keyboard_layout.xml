            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:soundEffectsEnabled="true"
            android:text="n"
            android:textColor="@color/darkgrey"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btn_m"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:layout_weight="1"
            android:background="@android:color/transparent"
            android:soundEffectsEnabled="true"
            android:text="m"
            android:textColor="@color/darkgrey"
            android:textSize="20sp" />

        <Button
            android:id="@+id/btn_backspace"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="6dp"
            android:background="@drawable/backspace"
            android:soundEffectsEnabled="true" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/level4"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:layout_below="@id/level3"
        android:weightSum="4">

        <Button
            android:id="@+id/btn_go"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="6dp"
            android:background="@drawable/dark_grey_buttons"
            android:text="Go"
            android:soundEffectsEnabled="true"
            android:src="@drawable/title1"
            android:visibility="gone"
            android:scaleType="fitXY"/>

        <Button
            android:id="@+id/btn_num_switch"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="6dp"
            android:background="@android:color/transparent"
            android:text="123"
            android:textColor="@color/darkgrey"
            android:textSize="15sp"
            android:tag="alphabetical"
            android:soundEffectsEnabled="true"/>

        <Button
            android:id="@+id/btn_spacebar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="0.8"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="2dp"
            android:layout_marginBottom="2dp"
            android:soundEffectsEnabled="true"/>

        <Button
            android:id="@+id/btn_dot"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1.2"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="4dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="6dp"
            android:background="@android:color/transparent"
            android:textSize="15sp"
            android:text="."
            android:textColor="@color/darkgrey"
            android:soundEffectsEnabled="true"
            android:visibility="visible"
            />



        <Button
            android:id="@+id/btn_enter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="25dp"
            android:layout_marginTop="6dp"
            android:layout_marginBottom="6dp"
            android:background="@drawable/enter"
            android:soundEffectsEnabled="true"
            />


    </LinearLayout>

</RelativeLayout>